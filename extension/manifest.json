{
	"name": "Force Background Tab",
	"version": "1.1",

	"description": "Force New Tab Running In Background",
  "icons": { "16": "assets/images/icon_16.png",
             "48": "assets/images/icon_48.png",
            "128": "assets/images/icon_128.png" },

	"background_page": "background.html",

	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	],

  "browser_action": {
      "default_title": "Force Background Tab",
      "default_icon": "assets/images/icon_16.png",
      "popup": "popup.html"
  }
}
